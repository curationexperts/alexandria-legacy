<% assets_with_deactivated_embargoes.each do |curation_concern| %>
  <%= link_to curation_concern.title, [:edit, curation_concern, :access] %>
  <%= curation_concern.admin_policy.title %>
  <ul>
    <% curation_concern.embargo_history.each do |entry| %>
      <% unless entry.empty? %>
        <li><%= entry %></li>
      <% end %>
    <% end %>
  </ul>
<% end %>
